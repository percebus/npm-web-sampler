{
  "name": "npm-web-sampler",
  "version": "0.1.0",
  "description": "npm packages sampler",
  "main": "Gruntfile.js",
  "scripts": {
    "pip:install:dev": "python -m pip install -r ./requirements.dev.txt",
    "pip:install:prd": "python -m pip install -r ./requirements.prd.txt",
    "pip:install:all": "python -m pip install -r ./requirements.txt",
    "npm:install:global": "npm install -g bower standard grunt-cli yarn",
    "setup": "npm run pip:install:all && npm run npm:install:global",
    "bower:install": "bower update --allow-root && bower install --allow-root",
    "yarn:licenses:list:prod": "yarn licenses list --prod > LICENSES.txt",
    "yarn:licenses:generate-disclaimer:prod": "yarn licenses generate-disclaimer --prod > LICENSES.md",
    "yarn:licenses:prod": "npm run yarn:licenses:list:prod && npm run yarn:licenses:generate-disclaimer:prod",
    "LICENSES": "npm run yarn:licenses:prod",
    "grunt:lint": "grunt lint --verbose --color",
    "lint": "npm run grunt:lint",
    "pretestem:ci": "npm run lint",
    "testem:ci": "testem ci --reporter xunit > testem.xunit.xml",
    "prejest:ci": "npm run lint",
    "jest:ci": "jest --ci",
    "jest:ci:unit": "npm run jest:ci -- unit.spec.js",
    "jest:ci:rest": "npm run jest:ci -- rest.spec.js",
    "test": "npm run jest:ci:unit",
    "validate": "npm run jest:ci:rest"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/percebus/npm-web-sampler.git"
  },
  "keywords": [
    "npm",
    "git",
    "html",
    "lint",
    "grunt",
    "jest",
    "frisby"
  ],
  "author": "percebus",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/percebus/npm-web-sampler/issues"
  },
  "homepage": "https://github.com/percebus/npm-web-sampler#readme",
  "devDependencies": {
    "frisby": "^2.1.3",
    "grunt": "^1.4.1",
    "grunt-shell": "^3.0.1",
    "grunt-yamllint": "^0.3.0",
    "jest": "^27.4.7",
    "load-grunt-tasks": "^5.1.0"
  }
}
